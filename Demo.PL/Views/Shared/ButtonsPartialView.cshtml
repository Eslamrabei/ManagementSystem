@model string

<td>
    <a asp-action="Details" asp-route-Id="@Model"
       class="btn btn-outline-primary btn-sm rounded-pill shadow-sm">
        <i class="fa-solid fa-eye"></i> Details
    </a>
</td>

<td>
    <a asp-action="Edit" asp-route-Id="@Model"
       class="btn btn-outline-success btn-sm rounded-pill shadow-sm">
        <i class="fa-solid fa-edit"></i> Edit
    </a>
</td>

<td>
    <button type="button" class="btn btn-outline-danger btn-sm rounded-pill shadow-sm" data-bs-toggle="modal" data-bs-target="#deleteModal-@Model">
        <i class="fas fa-trash"></i> Delete
    </button>
</td>
